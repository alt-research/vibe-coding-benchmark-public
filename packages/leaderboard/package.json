{
  "name": "@vibecodingbench/leaderboard",
  "version": "0.1.0",
  "description": "Leaderboard service for VibeCodingBench evaluations",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "lint": "eslint src/",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@hono/zod-validator": "^0.4.0",
    "@neondatabase/serverless": "^0.7.0",
    "drizzle-orm": "^0.29.0",
    "hono": "^4.11.10",
    "postgres": "^3.4.0",
    "vite": "7.3.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.17"
  }
}
