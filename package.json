{
  "name": "vibecodingbench",
  "version": "0.1.0",
  "description": "Benchmark for evaluating AI coding agents on real-world developer tasks",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "dev": "turbo dev",
    "clean": "turbo clean && rm -rf node_modules",
    "cli": "node packages/cli/dist/index.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "agentic-dev-starter": "^1.1.1",
    "turbo": "^2.3.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.53.1"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alt-research/vibe-coding-benchmark-public.git"
  },
  "keywords": [
    "benchmark",
    "ai",
    "coding",
    "agents",
    "evaluation",
    "claude",
    "gpt",
    "deepseek",
    "glm",
    "minimax"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "overrides": {
    "esbuild": "^0.25.0"
  }
